@model System.Collections.Generic.List<MultichoiceCollection.Presentation.Models.BouquetModel>

@{
    ViewBag.Title = "Bouquets";
}
<section class="row small-spacing">
    <div class="box-content">
        <h4 class="box-title">Bouquet Types</h4>

        <!-- /.dropdown js__dropdown -->
        <section class="table-responsive">
            <table class="table table-striped margin-bottom-10">
                <thead>
                    <tr>
                        <th>Code</th>
                        <th>Name</th>
                        <th>Description</th>
                        <th>Currency</th>
                        <th>Subscription</th>
                        <th>Name & Price</th>
                        <th>transaction Fee</th>
                        <th>Payment Url</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.code</td>
                            <td>@item.name</td>
                            <td>@item.description</td>
                            <td>@item.currency</td>
                            <td>@item.subscription</td>
                            <td>@item.nameAndPrice</td>
                            <td>@item.transFee</td>
                            <td>@item.paymentUrl</td>
                        </tr>
                    }


                </tbody>
            </table>
        </section>
        <!-- /.table -->
    </div>
</section>