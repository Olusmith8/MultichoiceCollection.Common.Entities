@model System.Collections.Generic.IEnumerable<MultichoiceCollection.Presentation.Models.BouquetTypeModel>
@{
    ViewBag.Title = "Bouquet Types";
}
<section class="row small-spacing">
    <div class="box-content">
        <h4 class="box-title">Bouquet Types</h4>
        
        <!-- /.dropdown js__dropdown -->
        <section class="table-responsive">
            <table class="table table-striped margin-bottom-10">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Bouquet Url</th>
                    </tr>
                </thead>
                <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.name</td>
                        <td><a href="#" target="_new">@item.bouquetUrl</a></td>

                    </tr>
                }
                

                </tbody>
            </table>
        </section>
        <!-- /.table -->
    </div>
</section>


