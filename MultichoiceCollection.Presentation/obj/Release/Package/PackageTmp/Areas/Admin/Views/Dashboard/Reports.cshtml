@model System.Collections.Generic.List<MultichoiceCollection.Common.Entities.Transaction>
@{
    ViewBag.Title = "Reports";
}
<section class="row small-spacing">
    <div class="box-content">
        <h4 class="box-title">Reports</h4>
        <!-- /.dropdown js__dropdown -->
        <section class="table-responsive">
            <table class="table table-striped margin-bottom-10">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Account Number</th>
                        <th>Amount</th>
                        <th>Fees</th>
                        <th>Customer Number</th>
                        <th>Phone</th>
                        <th>Trans Date</th>
                        <th>Audit Ref No</th>
                        <th>Device No</th>
                        <th>Business Type</th>
                        <th>Creation Date</th>
                    </tr>
                </thead>
                <tbody>
                    @{ var sn = 0;}
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>@(sn++)</td>
                        <td>@item.accountNumber</td>
                        <td>@item.amount</td>
                        <td>@item.transFees</td>
                        <td>@item.customerNumber</td>
                        <td>@item.mobileNumber</td>
                        <td>@item.transactionDate</td>
                        <td>@item.auditReferenceNo</td>
                        <td>@item.deviceNumber</td>
                        <td>@item.businessType</td>
                        <td>@item.CreatedDate</td>
                    </tr>
                    }

                </tbody>
            </table>
        </section>
        <!-- /.table -->
    </div>
</section>